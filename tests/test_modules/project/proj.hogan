<div class='project'>
  <img class='full' src='{{full.img}}' width={{full.w}} height={{full.h}}>
  </img>
  <div class='desc'>{{desc}}</div>
  <ul class='thumbs'>{{#thumbs}}
    <li>
      <a href='#{{title}}'>
        <img width={{w}} height={{h}} src='{{src}}'/>
      </a>
    </li>
  {{/thumbs}}</ul>
</div>
