body
  font-size 20px
  font-family "Akkurat Mono"
  background #fffff5
  background white
  margin 0
  padding 0
  letter-spacing 1px

input 
  font-size 18px
  font-family "Akkurat Mono"!important
  letter-spacing 1px
  background transparent
  margin 0
  padding 0
  line-height 22px
  border-radius 0
  outline none
  border none

input:hover
  color black

input:focus
  color black
  line-height 22px
  outline none
  border none

.login
  background rgba(212, 238, 255, 0.8)
  box-sizing border-box
  position absolute
  top 0
  left 0
  width 100%
  height 100%
  z-index 999

#loginForm
  line-height 20px
  color black
  position absolute
  top 50%
  left 50%
  margin-left -105px
  margin-top -20px
  width 210px
  cursor default
  z-index 999

.login input:focus
  color black

.login input 
  font-size 18px
  color black!important
  background rgba(212, 238, 255, 0.8)
  text-align center!important
  border 1px solid black
  border-bottom 2px solid black
  border-right 2px solid black
  width 200px

.omni
  line-height 20px
  color #333
  position absolute
  top 0
  left 0
  cursor default
  z-index 999
  width 100%
  height 61px
  padding 0
  margin 0
  line-height 24px
  position absolute
  border-bottom 2px solid black
  background #f1f1f1
  background #D4EEFF
  background rgba(212, 238, 255, 0.8)
  // background LightSkyBlue

.omni #mode
  -moz-appearance none
  -webkit-appearance none
  border none
  background transparent
  box-shadow none
  font-family "Akkurat Mono"
  text-indent 0.01px
  text-overflow ''

.omni a
  text-decoration none
  color black

.omni .menu 
  margin 10px 0 10px 10px
  padding 0
  padding-top 3px
  float left
  width 800px

.omni .menu li 
  z-index 999
  line-height 33px
  list-style none
  border 1px solid black
  border-bottom 2px solid black
  border-right 2px solid black
  float left
  padding 0 8px
  margin 0!important    
  font-family "Akkurat Mono"
  font-size 18px
  list-style none
  color black

.omni .filter
  color black!important
  border-right none!important
  text-transform uppercase
  padding-top 2px!important
  line-height 31px!important
  font-size 12px!important
  text-align center
  width 68px

.omni .messages 
  margin 0
  padding 0
  position absolute
  top 60px
  left 0
  width 100%
  background skyBlue
  background rgba(212, 238, 255, 0.9)
  border-bottom 2px solid black

.omni .messages li
  list-style none
  font-size 12px
  padding-left 11px
  color black
  text-transform uppercase

.omni .menu li:hover
  text-decoration underline

.omni #search
  width 320px
  line-height 28px
  border-left none
  color black

.omni .status ul li
  list-style none
  margin 0
  padding 0
  font-family "Akkurat Mono"
  font-size 16px
  line-height 22px
  color black

.omni .omnibox, .omni .actions
  text-transform uppercase
  font-size 12px!important
  border none!important
  padding-right 0px!important
  line-height 36px !important

.omni .actions
  color black!important

.omni .status ul
  margin 0
  padding 0
  list-style none

.project
  position absolute
  width 825
  margin-left -412.5px
  left 50%
  z-index 2

.project .opt_image_main
  background-size cover!important
  margin-bottom 10px
  width 825px
  height 400px

.project .main
  float left
  clear both

.project ul
  float left
  clear both
  margin 0
  padding 0

.project ul li
  list-style none
  padding 0 10px 0 0
  margin 0
  float left

.project ul li.l5
  padding 0 0 0 0

.project ul li div
  height 120px
  width 157px
  background-size cover!important

.project .opt_text_body
  float left
  clear both
  min-width 10px
  min-height 10px

.dropzone
  z-index 888!important
  background blue
  position absolute
  opacity 0.3
  box-shadow 0px 0px 30px blue

.edit.editBox
  position absolute
  z-index 999!important
  min-height 33px
  min-width 200px
  background #f1f1f1
  background #D4EEFF
  background rgba(212, 238, 255, 0.9)
  border 1px solid black
  border-bottom 2px solid black
  border-right 2px solid black
  box-shadow 0px 0px 30px skyBlue
  color black
  margin 0
  padding 6px

input[type="password"]::-webkit-input-placeholder
  color black!important
  opacity 0.4
