@import "fonts.css"

@media (min-width: 640px)
  body 

@media (min-width: 1024px)
  body

*, *:before, *:after
  -moz-box-sizing border-box
  -webkit-box-sizing border-box
  box-sizing border-box

svg 
  shape-rendering: crispEdges;

svg text::selection
  background #f1f1f1!important
  fill black!important

a
  color #080A17
  text-decoration none

body
  font-size 12px
  font-family "Andale Mono"
  margin 0
  padding 0

input, input:focus
  font-family "Andale Mono"!important
  color #080A17!important
  -webkit-font-smoothing subpixel-antialiased
  padding 0
  outline none
  background transparent!important
  border 1px solid #080A17

.omni, .login
  background rgba(255, 255, 255, 0.93)
  border  1px solid rgba(8, 10, 23, 0.29) 
  width 320px
  min-height 460px
  position fixed
  left 10px
  top 10px
  margin 0
  list-style none
  padding 8px 10px 10px 10px
  z-index 999
  font-family "Andale Mono"
  cursor -webkit-grab
  cursor grab

.omni .tray 
  list-style none
  margin 0 
  padding 0

.omni .tray li
  padding 2px 2px 10px 2px 
  margin 8
  margin-right 1px
  float left
//  box-shadow 0px 2px 13px #C3C2C2

.omni .blackbox
  position absolute
  bottom 10px
  left 10px

.omni .blackbox input
  background transparent
  border 1px dotted
  font-size 12px
  padding-left 2px
  padding 0 0 2px 2px
  height 16px
  width 268px

.omni .tray .xray
  margin-left 4px
  color #333
//  background rgba(6, 6, 19, 0.81)

.omni .tray .xray, .omni .tray .pallette
  float right
  line-height 16px
  font-size 24px
  border-radius 5px

.omni .exit
  position absolute
  bottom 0px
  right 12px
  font-size 30px
  color #080A17

.omni .tray
  cursor default

.omni .tray .canvasName
  width 234px
  border none
  border-bottom 2px solid
  color #080A17!important
  font-size 12px
  padding-left 2px
  padding 0 0 2px 2px
  height 16px

.omni .tray .pallette
  background #FAFAFA

.omni .tray .pallette span
  float left
  font-size 32px
  color #080A17!important

.omni .tray .pallette input
  color #080A17!important
  font-size 13px
  border none!important
  width 100px

.omni .save, .omni .open
  font-size 11px
  font-weight 600
  line-height 15px
  font-family "Andale Mono"
  background rgba(241, 241, 241, 0.9)
  border-radius 3px
  color #080A17!important
  text-align center
  border none
  outline none

.omni .lib
  cursor default
  background #FAFAFA
  list-style none
  margin-top 24px
  padding 14px
  border-radius 6px
  height 390px

.omni .lib li
  background white
  margin 2px 0 0 2px
  border 1px dotted black
  padding 0px 6px 0px 6px
  list-style none
  line-height 24px
  height 24px
  font-size 12px
  float left

.omni .lib span
  text-transform uppercase
  font-weight 600

.login 
  min-height 12px
  padding 4px

.login input
  font-size 12px
  width 310px
  border 1px dotted
  padding 0 4px

.login .error 
  color rgb(231, 2, 45)
  background rgba(255, 144, 168, 0.62)
  line-height 16px
  text-transform uppercase
  letter-spacing 1px
  padding 0 4px 0 4px
  margin 4px 0 0 0
  position absolute
  right 5px
  top 1px

.header
  float left 
  clear both
  height 80px
  width 100%

.header ul
  position relative
  padding 0
  margin 0
  margin-left -275px
  margin-top 50px
  left 50%
  width 550px
  list-style none

.header ul li
  padding 0
  margin 0
  margin-right 10px
  height 30px
  float left

.project
  float left
  clear both

.project .full
  float left

.project .desc
  float left

.project ul
  float left
  clear both
  list-style none
  margin 10px 0 0 0
  padding 0

.project ul li
  margin-right 10px
  float left

.category
  position relative
  margin-left -400px
  left 50%
  width 800px

.category ul
  list-style none
  margin 0
  padding 0

.category ul li
  padding 0
  margin 0

.category h1
  font-size 14px
  text-transform uppercase
  margin 0
  color #555

.article 
  position relative
  width 800px
  left 50%
  margin-left -400px

.article .txt
  width 400px
  margin-right 10px

.article img
  float right

#cvs
  position fixed
  z-index 9999
  top 46px
  left 10px
  border-radius 6px
  background rgba(6, 6, 19, 0.9)
  box-shadow 0px 2px 13px #C3C2C2
  display none

.grifter
  background rgba(8, 10, 23, 0.87)
  color #f1f1f1
  font-size 14px
  position absolute
  min-height 33px
  min-width 120px
  z-index 999!important
  margin 0
  padding 6px
  border 1px solid black
  border-bottom 2px solid black
  border-right 2px solid black
  box-shadow 0px 0px 30px #080A17

.pumicle
  float left

img
  border none
  background -webkit-gradient(
    linear,
    left top,
    left bottom,
    color-stop(0, #D69FB0),
    color-stop(1, #CDD4DE)
  )
  background -o-linear-gradient(bottom, #D69FB0 0%, #CDD4DE 100%)
  background -moz-linear-gradient(bottom, #D69FB0 0%, #CDD4DE 100%)
  background -webkit-linear-gradient(bottom, #D69FB0 0%, #CDD4DE 100%)
  background -ms-linear-gradient(bottom, #D69FB0 0%, #CDD4DE 100%)
  background linear-gradient(to bottom, #D69FB0 0%, #CDD4DE 100%)
