//   color rgba(240, 59, 148, 0.58)
//   font-size 14px
//   text-decoration line-through
//   padding 0 8px 2px
//   width inherit
//   float right
 
.grifter .tray 
  width 100%
  float left
  clear both
  font-size 10px
  text-align center
  color rgba(222, 111, 162, 0.83)

.pumicle
  float left

img
  border none
  background -webkit-gradient(
    linear,
    left top,
    left bottom,
    color-stop(0, #D69FB0),
    color-stop(1, #CDD4DE)
  )
  background -o-linear-gradient(bottom, #D69FB0 0%, #CDD4DE 100%)
  background -moz-linear-gradient(bottom, #D69FB0 0%, #CDD4DE 100%)
  background -webkit-linear-gradient(bottom, #D69FB0 0%, #CDD4DE 100%)
  background -ms-linear-gradient(bottom, #D69FB0 0%, #CDD4DE 100%)
  background linear-gradient(to bottom, #D69FB0 0%, #CDD4DE 100%)

.status
  background rgba(8, 10, 23, 0.94)
  color rgba(255, 255, 255, 0.79)!important
  width 100%
  height 20px
  font-size 14px
  line-height 18px
  padding 0 8px
  position fixed
  z-index 9999
  top 0
  left 0
