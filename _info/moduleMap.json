    {
      "id": "console",
      "scope": [
        "client"
      ],
      "desc": "cli",
      "deps": [
        "console.html",
        "console.styl"
      ],
      "filePath": "./_wilds/console.js",
      "styl": "form\n  top 0\n  left 0\n  position fixed\n  z-index 1\n  x-oveflow hidden\n  width 400px\n  height 16px\n  padding 5px\n\ninput\n  height 16px\n  line-height 16px\n  width inherit\n  background transparent\n  color #333\n  border none\n  font-size 15px\n  font-family 'ProggySquareTT'\n\ninput:focus\n  padding 2px\n  padding-top 4px\n  padding-left 4px\n  color blue\n  background transparent\n  background #f1f1f1\n  outline none\n  opacity 0.7\n  opacity 1\n  border none\n  border-bottom 1px solid blue\n",
      "html": "<form id='x'>\n  <input type='text' value='' class='console'>\n  <input type='submit' style='display:none;'>\n</form>\n"
    },
    {
      "id": "mdisp",
      "scope": [
        "client"
      ],
      "desc": "micro display",
      "deps": [
        "mdisp.html",
        "mdisp.styl"
      ],
      "filePath": "./_wilds/mdisp.js",
      "html": "<canvas id='mdisp'></canvas>\n",
      "styl": "#mdisp\n  z-index 0\n  position fixed\n  width 100%\n  height 100%\n  top 0 \n  left 0\n  background transparent\n"
    }
  ],
