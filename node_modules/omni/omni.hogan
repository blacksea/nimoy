<div class='omni'> 
  <ul class='menu'>
    <li class='command'></li>
    <li><input type='text' id='search' placeholder=''></input></li>
  </ul>
  <span class='result'></span>
  <ul class='messages'></ul>
  <ul class='commands'>{{#commands}}<li>{{txt}}</li>{{/commands}}</ul>
</div>

<span class="res">{{txt}}</span>
<span class='cmd'>{{txt}} :</span>
<span class="label">{{txt}}</span>

<ul class='ctx'>{{#options}}<li class="{{id}}">{{opt}}</li>{{/options}}</ul>

<textarea class='grifter edit editBox' 
  style="left:{{x}};top:{{y}};width:{{w}};height:{{h}};">
</textarea>

<li class='notification'>{{msg}}</li>

<div class='dropzone grifter' 
  style="left:{{x}};top:{{y}};width:{{w}};height:{{h}};">
</div>
