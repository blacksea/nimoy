body
  width 100%
  height 100%
  margin 0
  padding 0
  background #00001E
  overflow hidden
  position fixed
  font-size 8px
  letter-spacing 1px
  font-family 'monaco'
  color #fff

a
  color #00B7FF

 #number
	font-family 'monaco'
	font-size  9px
	cursor none
	width 50px
	height 12px
	border 1px solid #ccc
	color white
	float left
	text-align center

.row
	border-bottom 1px solid #ccc
	height 12px
	padding 6px

#grip 
	-webkit-user-drag: element
	height 8px
	background #ccc
	cursor crosshair

#grip:-webkit-drag
	border none 
	width 1px
	height 1px
	background-color #00001E

label
	font-size 9px
	text-transform uppercase
	line-height 15px
	padding-right 4px
	letter-spacing 0px
	float left
	color #ccc

@import 'ui'

